  "main": "lib/index.js",
  "scripts": {
    "build": "sucrase -q -t typescript,imports,jsx -d ./lib ./src",
    "built": "npm run build && npm run test",
    "clean": "rimraf lib",
    "demo": "parcel serve src/index.html --open",
    "fix": "npm run lint -- --fix",
    "go": "npm run build && npm run start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "precommit": "npm run build && npm run test",
    "prepare": "configure ./src/utils/config.ts && npm run build",
    "pretest": "npm run lint",
    "start:api": "node -r sucrase/register src/indexApi.ts",
    "start:engine": "node -r sucrase/register src/indexEngine.ts",
    "test": "mocha -r sucrase/register test/**/*.test.js"
  },
  "dependencies": {
